{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "./prisma/testing/prisma-test-environment.ts",
  "setupFilesAfterEnv": ["<rootDir>/prisma/testing/prisma-test-setup.ts"],
  "testMatch": ["<rootDir>/test/**/*.e2e-spec.{ts,js}"],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "./coverage-e2e"
}
